#include <stdio.h>

int main()
{
    char s[100],f[100];
    int size=0,i,j,count;
    printf("enter the string");
    scanf("%s",s);
    for(i=0;s[i]!='\0';i++)
    {
        f[i]=-1;
        size++;
    }
    for(i=0;i<size;i++)
    {
        count=1;
        for(j=i+1;j<size;j++)
        {
            if(s[i]==s[j])
            {
                count++;
                f[j]=0;
            }    
        }
        if(f[i]!=0)
        {
            f[i]=count;
        }
    }
    for(i=0;i<size;i++)
    {
        if(f[i]!=0)
        {
            printf("%c occurs %d times\n",s[i],f[i]);
        }
    }
    

    return 0;
}

